				<label text="Disused switch – switch blades have been locked" de.text="Stillgelegte Weiche – Weichenzungen sind festgelegt" />
				<label text="Temporary locks due to defects at the signal box or construction works are not mapped!" 
				    de.text="Vorübergehende Verschlüsse von Weichen wegen Stellwerksstörungen oder Bauarbeiten werden nicht gemappt!" />
				<label text="Abandoned switches are not mapped because they do not exist anymore." 
				    de.text="Vollständig ausgebaute Weichen werden nicht gemappt, da sie nicht mehr existieren." />
				<space />
				<key key="disused:railway"
					value="switch" />
				<key key="railway"
					value="" /> <!-- remove railway=switch -->
				<text key="ref"
					text="Switch reference"
					de.text="Weichennummer"
					default=""
					delete_if_empty="true" />
				<check key="railway:local_operated"
					text="Local operated"
					de.text="Ortsgestellt"
					default="off"
					delete_if_empty="true" />
				<check key="railway:switch:electric"
